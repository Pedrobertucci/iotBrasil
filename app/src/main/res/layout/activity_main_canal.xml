<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="bertucci.pedro.iotbrasil.Canal.MainCanal">

    <TextView
        android:id="@+id/textView21"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:paddingLeft="10dp"
        android:text="Configurações/Gateway/Configuracoes-gateway/Canal" />

    <CheckBox
        android:id="@+id/checkCanal1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="49dp"
        android:text="Canal 01 - 905.5 Mhz."
        android:textSize="24sp"
        android:layout_below="@+id/textView21"
        android:layout_centerHorizontal="true" />

    <CheckBox
        android:id="@+id/checkCanal2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/checkCanal1"
        android:layout_alignStart="@+id/checkCanal1"
        android:layout_below="@+id/checkCanal1"
        android:layout_marginTop="24dp"
        android:text="Canal 02 - 905.9 Mhz."
        android:textSize="24sp" />

    <CheckBox
        android:id="@+id/checkCanal3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/checkCanal2"
        android:layout_alignStart="@+id/checkCanal2"
        android:layout_below="@+id/checkCanal2"
        android:layout_marginTop="34dp"
        android:text="Canal 03 - 906.3 Mhz."
        android:textSize="24sp" />

    <CheckBox
        android:id="@+id/checkCanal4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/checkCanal3"
        android:layout_alignStart="@+id/checkCanal3"
        android:layout_below="@+id/checkCanal3"
        android:layout_marginTop="36dp"
        android:text="Canal 04 - 906.7 Mhz."
        android:textSize="24sp" />

    <CheckBox
        android:id="@+id/checkApagaCanal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/checkCanal4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="33dp"
        android:text="Apagar Canal"
        android:textSize="24sp" />

    <Button
        android:id="@+id/btnCanalSalvarEnviar"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="37dp"
        android:background="@drawable/butons"
        android:text="SALVAR / ENVIAR"
        android:textColor="@color/cardview_light_background"
        android:textSize="20sp" />

</RelativeLayout>
